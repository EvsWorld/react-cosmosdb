{
  "name": "express-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "bulma": "^0.5.0",
    "concurrently": "^3.6.0",
    "cookie-parser": "^1.4.3",
    "debug": "^3.0.0",
    "express": "^4.15.3",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.5",
    "morgan": "^1.8.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0",

    "cross-env": "^5.2.0",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "babel-node": "babel-node --presets=/*a*/ --ignore='foo|bar|baz'",
